{
  "version": 2,
  "builds": [
      {
          "src": "build/index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["build/**"] }
      }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "build/routes/index.routes.js" },
    { "src": "/(.*)", "dest": "build/index.js" }
  ]
}